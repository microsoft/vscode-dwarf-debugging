{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "nodejs-testing.extensions": [
    {
      "extensions": [
        "ts"
      ],
      "parameters": [
        "--import",
        "tsx"
      ]
    }
  ],
  "clangd.arguments": [
    "--query-driver=**/emcc,**/em++,**/clang,**/clang++"
  ],
  "[cpp]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.formatOnSave": false
  }
}